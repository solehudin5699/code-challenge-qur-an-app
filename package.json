  "scripts": {
    "dev": "concurrently -p none \"npm:lint:watch\" vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "esw src --ext .ts --ext .tsx --color",
    "lint:watch": "npm run lint -- -w",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "npm run test -- --coverage",
    "test:watch:cover": "npm run test:watch -- --coverage"
  },
